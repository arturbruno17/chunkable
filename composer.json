{
    "name": "posart/chunkable",
    "description": "A package to upload videos with chunks",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Posart\\Chunkable\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Artur Bruno",
            "email": "arturjuliao20@gmail.com"
        }
    ],
    "require": {
        "illuminate/http": "^10.43",
        "illuminate/console": "^10.43",
        "illuminate/support": "^10.43",
        "illuminate/filesystem": "^10.43",
        "illuminate/validation": "*"
    },
    "suggest": {
        "league/flysystem-aws-s3-v3": "^3.0",
        "league/flysystem-ftp": "^3.0",
        "league/flysystem-sftp-v3": "^3.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Posart\\Chunkable\\Providers\\ChunkUploadProvider"
            ]
        }
    }
}

{
    "name": "arturbruno17/laravel-chunk-upload",
    "description": "A package to upload videos with chunks",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Arturbruno17\\LaravelChunkUpload\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Artur Bruno",
            "email": "arturjuliao20@gmail.com"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^11.0"
    },
    "require": {
        "illuminate/http": "^10.43",
        "illuminate/console": "^10.43",
        "illuminate/support": "^10.43",
        "illuminate/filesystem": "^10.43",
        "illuminate/validation": "*"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Arturbruno17\\LaravelChunkUpload\\Providers\\ChunkUploadProvider"
            ]
        }
    }
}
